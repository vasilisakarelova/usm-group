{
    "name": "whiterussianstudio/kirby-kit",
    "description": "Whiter Russian Kirby Kit",
    "type": "project",
    "require": {
        "php": ">=7.3.0 <8.1.0",
        "getkirby/cms": "^3.5",
        "arnoson/kirby-vite": "^2.4",
        "bnomei/kirby3-php-cachedriver": "^1.3"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "extra": {
        "kirby-plugin-path": "app/plugins"
    }
}
